{
	"name": "@gymifie/pwa",
	"version": "1.0.0",
	"description": "The main frontend app",
	"scripts": {
		"serve": "parcel index.html --out-dir ./@build/output/serve/bundle --target browser --cache-dir ./@build/output/serve/cache --port 1234 --no-autoinstall --public-url /",
		"watch": "parcel watch index.html --out-dir ./@build/output/watch/bundle --target browser --cache-dir ./@build/output/watch/cache --no-autoinstall --public-url /",
		"build": "parcel build index.html --out-dir ./@build/output/build/bundle --target browser --no-source-maps --no-cache --public-url /"
	},
	"engines": {
		"node": "11.14.0"
	},
	"type": "module",
	"dependencies": {
		"@babel/runtime": "7.2.0",
		"v-state": "1.1.2",
		"vue": "2.6.10",
		"vue-clickaway": "2.2.2",
		"vue-router": "3.1.3",
		"vue-shortkey": "3.1.7",
		"vue-the-mask": "0.11.1",
		"vuex": "3.1.1",
		"winston": "3.2.1"
	},
	"devDependencies": {
		"@babel/core": "7.2.2",
		"@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
		"@babel/plugin-proposal-object-rest-spread": "7.2.0",
		"@babel/plugin-proposal-optional-chaining": "7.2.0",
		"@babel/plugin-syntax-import-meta": "7.2.0",
		"@babel/plugin-transform-regenerator": "7.0.0",
		"@babel/plugin-transform-runtime": "7.2.0",
		"@babel/preset-env": "7.5.2",
		"@vue/component-compiler-utils": "3.1.0",
		"del": "3.0.0",
		"gulp": "4.0.0",
		"parcel-bundler": "1.12.4",
		"vue-hot-reload-api": "2.3.4",
		"vue-template-compiler": "2.6.10"
	}
}
